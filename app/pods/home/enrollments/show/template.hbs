<ApplicationToolbar 
    @name={{model.enrollment.formattedName}}
    @previousRoute="home.classrooms.show" 
    @recordId={{model.enrollment.classroom.id}}/>

<article>
  <PaperList>
    {{#each nonCurrentLessons as |lesson|}}
      <Lessons::Tile 
        @type="type_one"
        @lesson={{lesson}}
        @enrollment={{model.enrollment}}
      />
    {{/each}}
  </PaperList>
</article>